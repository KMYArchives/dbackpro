	"description": "SQL Database Backup app",
	"scripts": {
		"js": "gulp js",
		"sass": "gulp sass",
		"start": "electron .",
		"builder": "yarn electron-builder",
		"js-nw": "gulp app engine plugins gui splash",
		"dpkg": "yarn remove gulp gulp-concat gulp-sass@4.1.1 node-sass gulp-babel @babel/core @babel/preset-env gulp-plumber gulp-uglify gulp-watch",
